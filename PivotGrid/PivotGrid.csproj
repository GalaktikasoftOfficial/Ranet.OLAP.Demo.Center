<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <PropertyGroup>
    <DemoModule>true</DemoModule>
    <DemoName>PivotGrid</DemoName>
    <ApplicationIcon />
    <StartupObject />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ranet.Olap.Mdx" Version="4.1.0-alpha228" />
    <PackageReference Include="Ranet.Olap.UI" Version="4.1.0-alpha228" />
    <PackageReference Include="Zaaml.UI" Version="0.2.1-alpha.23" />
  </ItemGroup>

  <ItemGroup>

    <EmbeddedResource Update="PivotGridResource.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>PivotGridResource.Designer.cs</LastGenOutput>
    </EmbeddedResource>

    <Resource Include="Resources\*.txt" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Ranet.Demo.Core\Ranet.Demo.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="PivotGridResource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>PivotGridResource.resx</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>